--11936_Query1
 SELECT 1 USERID  ,     
       --(SELECT FULLNAME FROM SEC_USER WITH(NOLOCK) WHERE USERID = 1 ) USERNAME,        
        ACS.STATEMENTDATE, ACS.FROMDATE, ACS.TODATE, ACS.COMMISSIONPERIOD,       
       CASE WHEN ACS.WHICHPAYMENT = 2 THEN 'TU'  WHEN ACS.COMMISSIONTYPE = 'T' THEN 'A' ELSE ACS.COMMISSIONTYPE END COMMISSIONTYPE,        
       ACS.ENTITYID AGENTID, ACS.AGENTNAME, ACS.AGENTCODE, ACS.WRITINGAGENTNAME,   
       ACS.WRITINGAGENTCODE, ACS.GAID, ACS.GANAME, ACS.GACODE, ACS.OPENINGBALANCE AGENTBALANCE,   
       ACS.CLOSINGAGENTADVANCEBALANCE UNEARNEDBALANCE, ACS.CARRIERNAME,       
       ACS.SHORTNAME, ACS.POLICYNUMBER, ACS.INSUREDNAME, ACS.INSUREDAGE, ACS.PRODUCTNAME,       
       ACS.ISSUEDATE, ACS.PAIDTODATE, ACS.PAYMODE,      
       CASE WHEN ACS.COMMISSIONPERIOD = 'F' THEN 'FY' WHEN ACS.COMMISSIONPERIOD = 'R' THEN 'RE' END DURATION,     
       ACS.PREMIUMAMT, ACS.COMMISSIONPERCENTAGE, ACS.PAIDATPERCENTAGE,    
       ACS.ORIGINALADVANCEBALANCE, ACS.COMMISSIONAMOUNT, 0 TYPEWISESUMMARY, ACS.UNEARNEDADVANCE,      
       ACS.POLICYUNEARNEDADVANCE, ACS.VOUCHERTYPE TYPE, ACS.VOUCHERCATEGORY SUBTYPE,       
       NULL TYPEDESCRIPTION, ACS.VOUCHERDATE, ACS.POLICYOPENUNEARNEDADVANCE,   
       ACS.PAYCODE, ACS.CONTRACTYEARS, ACS.EFFECTIVEDATE, ACS.APPLYTO,    
       ACS.FULLCOMMISSIONABLEPREMIUM, ACS.COMMPREMIUM, ACS.PERCENTAGEORFLAT,   
       ACS.VOUCHERDESCRIPTION   
       ,ACS.CLOSINGBALANCE_Rpt --added qc-6349 on 1/3/2013   
       ,ACS.CHANNELNAME -- 11504  
      FROM                        
       ARCHIVERPTCOMMISSIONSTATEMENT ACS WITH (NOLOCK)  
       --Added by Naveen Mungara for INS-8807  
       INNER JOIN CHANNEL CH WITH (NOLOCK) ON CH.CHANNELID = ACS.CHANNELID  
       --added by venubabuc as per INS-7094& INS-7093 on 12/21/2012  
       LEFT JOIN SYN_VUEFLATHIERARCHY H WITH (NOLOCK) ON ACS.ENTITYID= ISNULL(ISNULL(ISNULL(H.ENTITYID,SALESMGRID),AGENCYMGRID),ZONEMGRID)   
       AND H.ENTITYTYPEID = 500   
       AND H.ISPRIMARY = 1 WHERE 1=1  AND ACS.STATEMENTDATE = '2019-03-25'  AND ( ISNULL(H.AGENTCODE,'-1') = ('a48216' ) OR  
                 ISNULL(H.SALESMGRCODE,'-1') = ('a48216' ) OR  
                 ISNULL(H.AGENCYMGRCODE,'-1') = ('a48216' ) OR  
                 ISNULL(H.ZONEMGRCODE,'-1') = ('a48216' ) OR  
                ACS.AGENTCODE = ('a48216' )  
                 ) AND ACS.COMMISSIONTYPE <> 'U' AND ACS.COMMISSIONTYPE <> 'NA'
                 
                 --11936_Query2
                 
                       SELECT 1 USERID  ,     
       --(SELECT FULLNAME FROM SEC_USER WITH(NOLOCK) WHERE USERID = 1 ) USERNAME,        
        ACS.STATEMENTDATE, ACS.FROMDATE, ACS.TODATE, ACS.COMMISSIONPERIOD,       
       CASE WHEN ACS.WHICHPAYMENT = 2 THEN 'TU'  WHEN ACS.COMMISSIONTYPE = 'T' THEN 'A' ELSE ACS.COMMISSIONTYPE END COMMISSIONTYPE,        
       ACS.ENTITYID AGENTID, ACS.AGENTNAME, ACS.AGENTCODE, ACS.WRITINGAGENTNAME,   
       ACS.WRITINGAGENTCODE, ACS.GAID, ACS.GANAME, ACS.GACODE, ACS.OPENINGBALANCE AGENTBALANCE,   
       ACS.CLOSINGAGENTADVANCEBALANCE UNEARNEDBALANCE, ACS.CARRIERNAME,       
       ACS.SHORTNAME, ACS.POLICYNUMBER, ACS.INSUREDNAME, ACS.INSUREDAGE, ACS.PRODUCTNAME,       
       ACS.ISSUEDATE, ACS.PAIDTODATE, ACS.PAYMODE,      
       CASE WHEN ACS.COMMISSIONPERIOD = 'F' THEN 'FY' WHEN ACS.COMMISSIONPERIOD = 'R' THEN 'RE' END DURATION,     
       ACS.PREMIUMAMT, ACS.COMMISSIONPERCENTAGE, ACS.PAIDATPERCENTAGE,    
       ACS.ORIGINALADVANCEBALANCE, ACS.COMMISSIONAMOUNT, 0 TYPEWISESUMMARY, ACS.UNEARNEDADVANCE,      
       ACS.POLICYUNEARNEDADVANCE, ACS.VOUCHERTYPE TYPE, ACS.VOUCHERCATEGORY SUBTYPE,       
       NULL TYPEDESCRIPTION, ACS.VOUCHERDATE, ACS.POLICYOPENUNEARNEDADVANCE,   
       ACS.PAYCODE, ACS.CONTRACTYEARS, ACS.EFFECTIVEDATE, ACS.APPLYTO,    
       ACS.FULLCOMMISSIONABLEPREMIUM, ACS.COMMPREMIUM, ACS.PERCENTAGEORFLAT,   
       ACS.VOUCHERDESCRIPTION   
       ,ACS.CLOSINGBALANCE_Rpt --added qc-6349 on 1/3/2013   
       ,ACS.CHANNELNAME -- 11504  
      FROM                        
       ARCHIVERPTCOMMISSIONSTATEMENT ACS WITH (NOLOCK)  
       --Added by Naveen Mungara for INS-8807  
       INNER JOIN CHANNEL CH WITH (NOLOCK) ON CH.CHANNELID = ACS.CHANNELID  
       --added by venubabuc as per INS-7094& INS-7093 on 12/21/2012  
       LEFT JOIN SYN_VUEFLATHIERARCHY H WITH (NOLOCK) ON ACS.ENTITYID= ISNULL(ISNULL(ISNULL(H.ENTITYID,SALESMGRID),AGENCYMGRID),ZONEMGRID)   
       AND H.ENTITYTYPEID = 500   
       AND H.ISPRIMARY = 1 WHERE 1=1  AND ACS.STATEMENTDATE = '2019-03-18'  AND ( ISNULL(H.AGENTCODE,'-1') = ('a48216' ) OR  
                 ISNULL(H.SALESMGRCODE,'-1') = ('a48216' ) OR  
                 ISNULL(H.AGENCYMGRCODE,'-1') = ('a48216' ) OR  
                 ISNULL(H.ZONEMGRCODE,'-1') = ('a48216' ) OR  
                ACS.AGENTCODE = ('a48216' )  
                 ) AND ACS.COMMISSIONTYPE <> 'U' AND ACS.COMMISSIONTYPE <> 'NA'
